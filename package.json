  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch --env-file .env src/server.ts",
    "build": "tsup src --format cjs",
    "del:dist": "rm -rf dist",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
